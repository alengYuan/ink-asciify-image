{
  "name": "ink-asciify-image",
  "version": "1.0.0",
  "description": "Asciify image component for Ink.",
  "main": "dist/index.js",
  "types": "types/",
  "type": "module",
  "scripts": {
    "pre": "printf \"Initialize dependencies...\\n\"&&npm i&&printf \"\\n\"&&node ./tools/hack.mjs",
    "build": "printf \"🗑️ Clean up old files:\\n\\n\"&&rm -vrf ./dist ./types&&printf \"\\n💿 Initialize distributable files:\\n\\n\"&&babel ./src --out-dir ./dist --no-comments&&printf \"\\n🧩 Generate bundle files:\\n\"&&rollup -c&&printf \"\\n🔧 Fix buggy files:\\n\\n\"&&node ./tools/fix.mjs&&printf \"\\n💡 Generate type files:\\n\\n\"&&tsc ./src/index.js ./src/components/InkAsciifyImage/index.jsx ./src/utils/image.js --allowJs --declaration --emitDeclarationOnly --esModuleInterop --extendedDiagnostics --module nodenext --moduleResolution nodenext --outDir types --removeComments"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alengYuan/ink-asciify-image.git"
  },
  "keywords": [
    "ink",
    "ink-component",
    "asciify-image",
    "ascii",
    "image",
    "esm"
  ],
  "author": "Aleng Yuan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alengYuan/ink-asciify-image/issues"
  },
  "homepage": "https://github.com/alengYuan/ink-asciify-image#readme",
  "devDependencies": {
    "@babel/cli": "^7.22.10",
    "@babel/preset-react": "^7.22.5",
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-terser": "^0.4.3",
    "@types/node": "^20.5.7",
    "@types/react": "^18.2.21",
    "chalk": "^5.3.0",
    "eslint": "^8.48.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jimp": "^0.22.10",
    "prettier-eslint": "^15.0.1",
    "rollup": "^3.28.1",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "ink": ">=4.3.1",
    "react": ">=18.2.0",
    "synckit": ">=0.8.5"
  },
  "overrides": {
    "xml2js": "^0.5.0"
  }
}
